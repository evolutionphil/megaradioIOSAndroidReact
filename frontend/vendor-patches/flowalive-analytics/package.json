{
  "name": "flowalive-analytics",
  "version": "0.0.32",
  "description": "Mobile analytics SDK",
  "author": "Flowalive Analytics",
  "license": "AGPL-3.0-only",
  "keywords": [
    "mobile",
    "analytics",
    "expo",
    "react-native",
    "typescript",
    "web"
  ],
  "sideEffects": false,
  "exports": {
    "./expo": {
      "types": "./dist/mobile/expo/index.d.ts",
      "default": "./dist/mobile/expo/index.js"
    },
    "./react-native": {
      "types": "./dist/mobile/react-native/index.d.ts",
      "default": "./dist/mobile/react-native/index.js"
    },
    "./web": {
      "types": "./dist/web/index.d.ts",
      "default": "./dist/web/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:web": "tsup --entry.web src/web/index.ts",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch",
    "dev:web": "tsup --entry.web src/web/index.ts --watch"
  },
  "dependencies": {
    "pako": "^2.1.0",
    "socket.io-client": "^4.8.3",
    "ulidx": "^2.4.1"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": ">=1.0.0",
    "@react-native-community/netinfo": ">=11.0.0",
    "@react-navigation/native": ">=6.0.0",
    "expo-device": ">=6.0.0",
    "expo-localization": ">=14.0.0",
    "expo-router": ">=3.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.70.0",
    "react-native-device-info": ">=10.0.0",
    "react-native-localize": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "@react-native-community/netinfo": {
      "optional": true
    },
    "expo-device": {
      "optional": true
    },
    "expo-localization": {
      "optional": true
    },
    "expo-router": {
      "optional": true
    },
    "@react-native-async-storage/async-storage": {
      "optional": true
    },
    "react-native-device-info": {
      "optional": true
    },
    "react-native-localize": {
      "optional": true
    },
    "@react-navigation/native": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/pako": "^2.0.4",
    "@types/react": "^19.1.8",
    "@typescript/native-preview": "^7.0.0-dev.20251214.1",
    "expo-router": "^6.0.19",
    "tsup": "^8.3.5",
    "typescript": "^5"
  }
}
